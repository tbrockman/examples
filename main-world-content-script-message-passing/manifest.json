{
    "name": "Reddit tracking data mangler",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "host_permissions": [
        "https://*.reddit.com/*"
    ],
    "permissions": [
        "tabs",
        "scripting",
        "activeTab"
    ],
    "action": {
        "default_title": "Reddit tracking data mangler",
        "default_popup": "popup.html"
    },
    "externally_connectable": {
        "matches": [
            "https://*/*"
        ]
    }
}